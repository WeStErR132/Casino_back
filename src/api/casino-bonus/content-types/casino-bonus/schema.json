      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "What_we_dont_like": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "General": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
